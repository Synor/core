{
  "name": "synor",
  "version": "0.2.0",
  "description": "Database Migration Library",
  "keywords": [
    "database",
    "migration"
  ],
  "homepage": "https://github.com/MunifTanjim/synor#readme",
  "bugs": "https://github.com/MunifTanjim/synor/issues",
  "license": "MIT",
  "author": "Munif Tanjim (https://muniftanjim.dev)",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/MunifTanjim/synor",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all -p build:core build:databases build:sources",
    "build:core": "bili",
    "build:databases": "npm-run-all -p build:database:*",
    "build:database:mysql": "SYNOR_DATABASE=mysql bili",
    "build:sources": "npm-run-all -p build:source:*",
    "build:source:file": "SYNOR_SOURCE=file bili",
    "clean": "rimraf lib/*",
    "prepare": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "lodash.defaultsdeep": "^4.6.1",
    "mysql": "^2.17.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.19",
    "@types/lodash.defaultsdeep": "^4.6.6",
    "@types/mysql": "^2.15.8",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "bili": "^4.8.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-standard-with-typescript": "^10.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "rimraf": "^3.0.0",
    "rollup-plugin-typescript2": "^0.25.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2"
  }
}
